clear all
y=[1 10 2 3 5 4 55];
x=[1 23 3 5 2 52 3];
npoint = 20;
subplot(3,2,1);
stem(x);
title('Signal X');
subplot(3,2,2);
stem(y);
title('Signal Y');
yt=fft(y,npoint);
xt=fft(x,npoint);
subplot(3,2,3);
stem(yt);
title('fourier transform of Y');
subplot(3,2,4);
stem(xt);
title('fourier transform of X');
n=conv(x,y);
subplot(3,2,5);
stem(fft(n,npoint));
title('fourier transform of X');
subplot(3,2,6);
temp=xt.*yt;
stem(temp);